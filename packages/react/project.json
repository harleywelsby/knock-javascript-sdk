{
  "name": "react-headless",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/react-headless/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/react-headless/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "buildCJS": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "minify": false,
        "sourcemap": true,
        "outputPath": "dist/packages/react-headless/dist/cjs"
      }
    },
    "buildESM": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/react-headless/dist/esm/"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["buildESM", "buildCJS"],
      "options": {
        "command": "echo done building"
      }
    },
    "package": {},
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/react-headless"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/react-headless"
      }
    }
  }
}
